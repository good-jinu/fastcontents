{
	"name": "fastcontents-monorepo",
	"version": "0.0.0",
	"description": "monorepo for FastContents",
	"private": true,
	"type": "module",
	"scripts": {
		"fmt": "biome format --write && pnpm run -r format",
		"check": "biome check && pnpm run -r check",
		"check:fix": "biome check --write",
		"check:fix:unsafe": "biome check --write --unsafe",
		"test": "vitest --run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"build": "pnpm -r build"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.18.2",
	"engines": {
		"pnpm": ">=10.0.0",
		"node": ">=24.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.5",
		"@changesets/cli": "^2.29.7",
		"@vitest/ui": "^2.1.8",
		"vitest": "^2.1.8"
	}
}
